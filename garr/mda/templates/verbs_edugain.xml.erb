<bean id="serializeImportedEduGainUnsigned" parent="SerializationStage" p:id="serializeImportedEduGainUnsigned">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-edugain/<%= fedid_downcase %>-to-edugain-metadata.xml"/>
        </bean>
    </property>
</bean>

<bean id="serializeImportedEduGainSHA1Signed" parent="SerializationStage" p:id="serializeImportedEduGainSHA1Signed">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-edugain/<%= fedid_downcase %>-to-edugain-metadata-sha1.xml"/>
