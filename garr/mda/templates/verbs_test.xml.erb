<bean id="<%= fedid_downcase %>-test-check" parent="SimplePipeline" p:id="<%= fedid_downcase %>-test-check">
    <property name="stages">
        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_testEntities"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_checkMetadata"/>
            <ref bean="standardImportActions"/>
            <ref bean="errorAnnouncer"/>
