<bean id="serializeImportedTestUnsigned" parent="SerializationStage"
    p:id="serializeImportedTestUnsigned">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-test/<%= fedid_downcase %>-test-metadata.xml"/>
        </bean>
    </property>
</bean>

