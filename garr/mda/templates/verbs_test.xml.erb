<bean id="serializeImportedTestUnsigned" parent="SerializationStage" p:id="serializeImportedTestUnsigned">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-test/<%= fedid_downcase %>-test-metadata.xml"/>
        </bean>
    </property>
</bean>

<bean id="serializeImportedTestSHA1Signed" parent="SerializationStage" p:id="serializeImportedTestSHA1Signed">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-test/<%= fedid_downcase %>-test-metadata-sha1.xml"/>
        </bean>
    </property>
</bean>

<bean id="serializeImportedTestSHA256Signed" parent="SerializationStage" p:id="serializeImportedTestSHA256Signed">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-test/<%= fedid_downcase %>-test-metadata-sha256.xml"/>
        </bean>
    </property>
</bean>

<bean id="<%= fedid_downcase %>-test-check" parent="SimplePipeline" p:id="<%= fedid_downcase %>-test-check">
    <property name="stages">
        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_testEntities"/>
            <ref bean="standardImportActions"/>
            <ref bean="errorAnnouncer"/>
        </list>
    </property>
</bean>

<bean id="<%= fedid_downcase %>-test-metadata" parent="SimplePipeline" p:id="<%= fedid_downcase %>-test-metadata">
    <property name="stages">
        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_testEntities"/>
            <ref bean="standardImportActions"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_importTestTail"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_addTrustRoots"/>
            <ref bean="stripUkfedlabelNamespace"/>
            <ref bean="stripWayfNamespace"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_formatMetadata"/>
            <ref bean="serializeImportedTestUnsigned"/>
        </list>
    </property>
</bean>

<bean id="<%= fedid_downcase %>-test-metadata-sha1" parent="SimplePipeline" p:id="<%= fedid_downcase %>-test-metadata-sha1">
    <property name="stages">
        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_testEntities"/>
            <ref bean="standardImportActions"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_importTestTail"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_addTrustRoots"/>
            <ref bean="stripUkfedlabelNamespace"/>
            <ref bean="stripWayfNamespace"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_formatMetadata"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_signSHA1" />
            <ref bean="serializeImportedTestSHA1Signed"/>
        </list>
    </property>
</bean>

<bean id="<%= fedid_downcase %>-test-metadata-sha256" parent="SimplePipeline" p:id="<%= fedid_downcase %>-test-metadata-sha256">
    <property name="stages">
        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_testEntities"/>
            <ref bean="standardImportActions"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_importTestTail"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_addTrustRoots"/>
            <ref bean="stripUkfedlabelNamespace"/>
            <ref bean="stripWayfNamespace"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_formatMetadata"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_signSHA256" />
            <ref bean="serializeImportedTestSHA256Signed"/>
        </list>
    </property>
</bean>
