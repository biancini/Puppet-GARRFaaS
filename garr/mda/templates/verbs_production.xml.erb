<bean id="importProduction" parent="SimplePipeline"
    p:id="importProduction">
    <property name="stages">
        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_productionEntities_by_url"/>
            <ref bean="standardImportActions"/>
            <ref bean="standardImportTail"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_addTrustRoots"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_signSHA1EntitiesDescriptor"/>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_formatMetadata" />
            <ref bean="serializeImported"/>
        </list>
    </property>
</bean>

<bean id="importProductionRaw" parent="SimplePipeline"
