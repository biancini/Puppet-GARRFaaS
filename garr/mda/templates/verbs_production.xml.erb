        <list>
            <ref bean="<%= fedcountry_downcase %>_<%= fedid_downcase %>_ProdEntities"/>
            <ref bean="standardImportActions"/>
            <ref bean="errorAnnouncer"/>
        </list>
    </property>
</bean>

