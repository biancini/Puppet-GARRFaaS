<bean id="serializeImportedProductionUnsigned" parent="SerializationStage"
        p:id="serializeImportedProductionUnsigned">
    <property name="outputFile">
        <bean class="java.io.File">
            <constructor-arg value="#{ systemProperties['basedir'] }/mdx/<%= fedcountry_downcase %>_<%= fedid_downcase %>/md-out/<%= fedid_downcase %>-prod/<%= fedid_downcase %>-metadata.xml"/>
        </bean>
    </property>
</bean>

